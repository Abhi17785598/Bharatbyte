# Rename this file to `.env` and fill in your EmailJS credentials
# You can get these from https://dashboard.emailjs.com/

VITE_EMAILJS_SERVICE_ID=your_service_id
VITE_EMAILJS_TEMPLATE_ID=your_template_id
VITE_EMAILJS_PUBLIC_KEY=your_public_key

# Getform endpoints (optional)
# If set, PartnerFormModal will submit to this endpoint. Falls back to VITE_GETFORM_ENDPOINT or a hardcoded default.
VITE_GETFORM_PARTNER_ENDPOINT=
# General fallback used by other parts (e.g., Apply Now form default is hardcoded in its HTML)
VITE_GETFORM_ENDPOINT=

# Backend SMTP configuration (used by Express server for direct email sending)
# Required: set these to your SMTP provider (e.g., Gmail/Outlook/Zoho/SendGrid SMTP)
SMTP_HOST=smtp.example.com
SMTP_PORT=587
SMTP_SECURE=false
SMTP_USER=your_smtp_username
SMTP_PASS=your_smtp_password

# From/To addresses
# MAIL_FROM defaults to SMTP_USER if not set
# MAIL_TO is where submissions are sent; defaults to SMTP_USER if not set
MAIL_FROM=
MAIL_TO=

# Optional: custom server port (default 3001)
PORT=3001
